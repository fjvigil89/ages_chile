<?xml version="1.0"?>
<odoo>
    <data>

        <record id="view_account_invoice_form_inherit_integration" model="ir.ui.view">
            <field name="name">view.account.invoice.form.inherit.integration</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_form"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='invoice_line_ids']/tree//field[@name='quantity']"
                           position="after">
                        <field name="quantity_receive"/>
                         <field name="check_done" invisible="1"/>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</odoo>